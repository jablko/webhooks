        "license",
        "forks",
        "open_issues",
        "watchers",
        "default_branch",
        "public"
      ],
      "properties": {
        "id": { "type": "integer" },
        "node_id": { "type": "string" },
        "name": { "type": "string" },
        "full_name": { "type": "string" },
        "private": { "type": "boolean" },
        "owner": { "$ref": "common/user.schema.json" },
        "html_url": { "type": "string", "format": "uri" },
        "description": { "type": ["string", "null"] },
